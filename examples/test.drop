class A {
    fn moo() {
        print("Hello World")
    }
}

class Point: A {
    x: float
    y: float

    new(x: float, y: float) {
        println("Hello")
        self.x = x
        self.y = y
    }

    fn distance() -> float {
        return (self.x * self.x + self.y * self.y)
    }

    fn add(other: Point) -> Point {
        return Point(self.x + other.x, self.y + other.y)
    }
}

fn main() {
    let p1 = Point(3.0, 4.0)
    let p2 = Point(1.0, 2.0)
    let p3 = p1.add(p2)

    let dist = p1.distance()
    println(dist)

    let m = p3.y

    println(m)
    // Or use println directly
    println("Distance:", dist)
    println("Point p3:", p3.x, p3.y)

    p1.moo()
}