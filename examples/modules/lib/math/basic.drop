mod math.basic

fn add(a: int, b: int) -> int {
    return a + b
}

fn subtract(a: int, b: int) -> int {
    return a - b
}

fn multiply(a: int, b: int) -> int {
    return a * b
}

fn divide(a: int, b: int) -> float {
    return a / b
}

fn square(x: int) -> int {
    return x * x
}

fn cube(x: int) -> int {
    return x * x * x
}