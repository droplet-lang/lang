// The base error class
class Error {
    message: str

    new(message: str) {
        self.message = message
    }
}

fn testError(x: int) -> str! {
    if(x == 1) {
        return Error("Sulav is a goood boy")
    }

    return "Hello"
}


fn main() {
    let result = testError(1)
    if result is Error {
        // handle error
        println(result.message)
        exit(100)
    }

    // or walk normally
    println(result)
}